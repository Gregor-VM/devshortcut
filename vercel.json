{
  "version": 2,
  "installCommand": "npm install && npm run build",
  "buildCommand": "npm run build",
  "devCommand": "npm run preview",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" }
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    { "src": "*.html", "use": "@vercel/static" },
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ]}
