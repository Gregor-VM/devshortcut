{
  "version": 2,
  "installCommand": "npm install && npm run build",
  "buildCommand": "npm run build",
  "devCommand": "npm run preview",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    { "src": "*.html", "use": "@vercel/static" },
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
